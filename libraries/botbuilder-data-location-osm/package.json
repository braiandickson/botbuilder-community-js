{
  "name": "@botbuildercommunity/data-location-osm",
  "version": "1.0.0",
  "description": "Bot Builder Community convenience package for interacting with Open Street Maps data for geolocation and conversion.",
  "main": "./lib/index.js",
  "typings": "index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "exclude": [
      "**/*.d.ts",
      "**/*.test.js"
    ],
    "reporter": [
      "text",
      "cobertura",
      "html"
    ],
    "report-dir": "./.nyc_output/coverage"
  },
  "scripts": {
    "build": "tsc",
    "test": "tsc && nyc mocha",
    "test-ci": "tsc && nyc mocha --reporter xunit --reporter-options output=./mocha-test-results.xml --reporter-options suiteName=$npm_package_name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BotBuilderCommunity/botbuilder-community-js.git"
  },
  "keywords": [
    "chatbots",
    "bot framework",
    "bot builder",
    "us census",
    "location",
    "geolocation"
  ],
  "contributors": [
    "Bot Builder Community",
    "Michael Szul <michael@szul.us>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BotBuilderCommunity/botbuilder-community-js/issues"
  },
  "homepage": "https://github.com/BotBuilderCommunity/botbuilder-community-js#readme",
  "dependencies": {
    "request": "^2.88.0",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/qs": "^6.9.0",
    "@types/request": "^2.48.3",
    "@types/request-promise": "^4.1.45",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "rewire": "^4.0.1",
    "typescript": "^3.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
